https://www.hackerrank.com/challenges/a-very-big-sum

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main()
{
	int n, *arr;
	long ans=0;
	cin>>n;
	arr=new int[n];
	for(int i=0; i<n; i++)
		{
			cin>>arr[n];
			ans+=arr[n];
		}
	cout<<ans;
	return 0;
}
